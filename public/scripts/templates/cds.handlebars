<<div id="cds" class="items">
    {{#each this}}
        <div class="item-container">
            <div>
                <ul class="item-properties">
                    <li><a href="#/products/cds/{{this.id}}"><h3>{{this.name}} - {{this.artist}}</h3></a></li>
                    <li>${{this.price}}</li>
                </ul>
              <img src="{{this.img}}" width="150">

            </div>
        </div>
    {{/each}}
</div>
